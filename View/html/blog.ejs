<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/main.css" >
</head>
<body>
    <%- include('includes/nav.ejs') %>

    <main class="centered">
        <% blogData.forEach((blogPost, index) => { %> 
            <a href="?page=<%= index + 1 %>"><%= index + 1 %></a>
        <% }); %>
        
        <% blogData.forEach(blogPost => { %> 
            <h1><%= blogPost.blogTitle %></h1>
            <p class="justified"><%= blogPost.blogContent %></p>
        <% }); %>
    </main>

</body>
</html>